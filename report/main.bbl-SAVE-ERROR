      \verb C\:\\Users\\alexa\\Zotero\\storage\\SFM2W7T6\\Garg et al. - 2025 - Approximating Competitive Equilibrium by Nash Welfare.pdf;C\:\\Users\\alexa\\Zotero\\storage\\8I4F3KVF\\2402.html
      \endverb
      \verb{urlraw}
      \verb http://arxiv.org/abs/2402.09994
