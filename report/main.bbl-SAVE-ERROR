    \entry{horodecki_quantum_2009}{article}{}
      \name{author}{4}{}{%
        {{hash=9b7133dfee0849cbeeeb7f2f83313774}{%
           family={Horodecki},
           familyi={H\bibinitperiod},
           given={Ryszard},
