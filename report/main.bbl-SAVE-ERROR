      \strng{namehash}{4076e9dac56e80c8a8a1133491503b5a}
      \strng{fullhash}{31f28f12dcf7389dabda01c45af76ca4}
      \strng{bibnamehash}{4076e9dac56e80c8a8a1133491503b5a}
      \strng{authorbibnamehash}{4076e9dac56e80c8a8a1133491503b5a}
